#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n,i;
    cin>>n;
    int arr[n],arr1[n];
    for(i=0;i<n;i++)
    cin>>arr[i];
    for(i=0;i<n;i++)
    {
        cin>>arr1[i];
        arr1[i]+=arr[i];
    }
    for(i=0;i<n;i++)
    cout<<arr1[i]<<" ";
    cout<<endl;
	return 0;
}

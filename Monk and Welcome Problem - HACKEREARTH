#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n,i;
    cin>>n;
    int arr[n],arr1[n],arr2[n];
    for(i=0;i<n;i++)
    cin>>arr[i];
    for(i=0;i<n;i++)
    cin>>arr1[i];
    for(i=0;i<n;i++)
    arr2[i]=arr[i]+arr1[i];
    for(i=0;i<n;i++)
    cout<<arr2[i]<<" ";
    cout<<endl;
	return 0;
}
